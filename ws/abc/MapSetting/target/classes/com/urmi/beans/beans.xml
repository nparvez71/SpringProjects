<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="contactbook" class="com.urmi.file.ContactBook">
		<property name="contacts">
			<map>
				<entry key="first" value-ref="contact1"></entry>
				<entry key="second" value-ref="contact2"></entry>
				<entry key="third" value-ref="contact3"></entry>
				<entry key="fourth" value-ref="contact4"></entry>
			</map>
		</property>
	</bean>

	<bean id="contact1" class="com.urmi.file.Contact">
		<property name="name" value="shamim"></property>
		<property name="number" value="1"></property>
	</bean>
	
	<bean id="contact2" class="com.urmi.file.Contact">
		<property name="name" value="urmi"></property>
		<property name="number" value="2"></property>
	</bean>
	
	<bean id="contact3" class="com.urmi.file.Contact">
		<property name="name" value="sohan"></property>
		<property name="number" value="3"></property>
	</bean>
	
	<bean id="contact4" class="com.urmi.file.Contact">
		<property name="name" value="parvez"></property>
		<property name="number" value="4"></property>
	</bean>
</beans>
